<h1>Liste des modèles :</h1>

<table border="8">

    <tr>
        <th>Id </th>
        <th>Libellé </th>
        <th>Pays </th>
        <th>Modifier </th>
        <th>Supprimer </th>


    </tr>

    {% for modele in modeles %}
        <tr>
            <td> {{ modele.id  }}</td>
            <td> {{ modele.libelle }}</td>
            <td> {{ modele.pays }}</td>
            <td><a href="{{ path("esprit_esprit_parc_modifier_Modele",{'id':modele.id}) }}">Modifier</a></td>
            <td><a href="{{ path("esprit_esprit_parc_supprimer_Modele",{'id':modele.id}) }}">Supprimer</a></td>
        </tr>
    {% endfor %}

</table>